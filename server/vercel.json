{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
           }
    ],
    "routes": [
        {
            "src": "/Course",
            "dest": "routes/Course.js"
        },
        {
            "src": "/Profile",
            "dest": "routes/Profile.js"
        },
        {
            "src": "/User",
            "dest": "routes/User.js"
        },
        {
            "src": "/Payment",
            "dest": "routes/Payment.js"
        },
        {
            "src": "/Contact",
            "dest": "routes/Contact.js"
        },
        {
            "src": "/search",
            "dest": "routes/search.js"
        }
    ]
}
